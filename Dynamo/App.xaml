<Application x:Class="Dynamo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Dynamo"
             xmlns:ngt="NodeGraph.Themes"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Dynamo;component/Themes/Generic.xaml"/>
                <ResourceDictionary Source="/Dynamo;component/Themes/Default.xaml"/>
                <ResourceDictionary Source="/Dynamo;component/Themes/PropertyEditors.xaml"/>
                <ResourceDictionary Source="/NodeGraph;component/Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
